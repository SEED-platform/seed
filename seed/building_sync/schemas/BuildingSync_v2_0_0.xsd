            <xs:element name="HeatExchanger" minOccurs="0">
              <xs:complexType/>
            </xs:element>
            <xs:element name="Other" type="auc:OtherType" minOccurs="0"/>
            <xs:element name="Unknown" type="auc:UnknownType" minOccurs="0"/>
