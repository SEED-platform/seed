              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:enumeration value="Continuous"/>
                  <xs:enumeration value="Temperature"/>
                  <xs:enumeration value="Timer"/>
                  <xs:enumeration value="Demand"/>
