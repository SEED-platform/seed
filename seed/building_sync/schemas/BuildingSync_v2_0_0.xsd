            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element ref="auc:ThirdPartyCertification" minOccurs="0"/>
      <xs:element ref="auc:YearInstalled" minOccurs="0"/>
      <xs:element ref="auc:YearOfManufacture" minOccurs="0"/>
      <xs:element ref="auc:Manufacturer" minOccurs="0"/>
