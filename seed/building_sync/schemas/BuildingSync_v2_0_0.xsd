                </xs:complexType>
              </xs:element>
              <xs:element name="Complex" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="StreetNumberPrefix" type="xs:string" minOccurs="0">
                      <xs:annotation>
                        <xs:documentation>The portion of the complete address number which precedes the Address Number itself.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
